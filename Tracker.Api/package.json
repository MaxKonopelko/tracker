{
  "name": "charity-management-system",
  "version": "1.0.0",
  "repository": "N/A",
  "license": "MIT",
  "engineStrict": false,
  "scripts": {
    "build": "webpack --mode production",
    "start": "concurrently --kill-others \"yarn watch\" \"yarn sass-watch\"",
    "lint": "tslint --project tsconfig.json",
    "sass-build": "yarn node-sass wwwroot/sass/ -o wwwroot/css/",
    "sass-watch": "yarn sass-build && yarn node-sass -w wwwroot/sass/ -o wwwroot/css/",
    "dev": "--mode development --open --hot",
    "watch": "webpack --mode development --devtool=inline-source-map --watch"
  },
  "devDependencies": {
    "@types/react": "16.8.10",
    "@types/react-bootstrap": "0.32.17",
    "@types/react-dom": "16.8.3",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "tslib": "1.9.2",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "4.3.1",
    "compass": "0.1.1",
    "compass-mixins": "0.12.10",
    "concurrently": "4.0.1",
    "node-sass": "4.9.3",
    "react": "16.8.6",
    "react-bootstrap": "1.0.0-beta.6",
    "react-dom": "16.8.6",
    "react-router-dom": "4.3.1",
    "ts-loader": "5.3.3",
    "webpack-livereload-plugin": "^2.2.0"
  }
}
