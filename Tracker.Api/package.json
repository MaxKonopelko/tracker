{
  "name": "charity-management-system",
  "version": "1.0.0",
  "repository": "N/A",
  "license": "MIT",
  "engineStrict": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "start": "concurrently --kill-others \"yarn dev\" \"yarn sass-watch\"",
    "lint": "tslint --project tsconfig.json",
    "sass-build": "yarn node-sass wwwroot/sass/ -o wwwroot/css/",
    "sass-watch": "yarn sass-build && yarn node-sass -w wwwroot/sass/ -o wwwroot/css/",
    "dev": "cross-env NODE_ENV=development rollup -cw"
  },
  "devDependencies": {
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "cross-env": "5.2.0",
    "rollup": "0.59.4",
    "rollup-plugin-cleanup": "3.0.0-beta.1",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-serve": "0.4.2",
    "rollup-plugin-typescript": "0.8.1",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-watch": "4.3.1",
    "shx": "0.2.2",
    "tslib": "1.9.2",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compass": "0.1.1",
    "compass-mixins": "0.12.10",
    "concurrently": "4.0.1",
    "node-sass": "4.9.3",
    "react": "^16.7.0",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1"
  }
}
